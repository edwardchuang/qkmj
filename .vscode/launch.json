{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Attach to Process",
            "type": "cppdbg",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "MIMode": "lldb",
            "program": "${workspaceFolder}/build/qkmj",
            "logging": {
                "engineLogging": true
            }
        },
        {
            "name": "(lldb) Launch mjgps (Server)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/mjgps",
            "args": ["7001"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}